    "code": {
      "type": "integer",
      "default": 0,
      "title": "The code Schema",
      "examples": [
        200
      ]
    },
    "data": {
      "type": "object",
      "default": {},
      "title": "The data Schema",
      "required": [
        "fullname",
        "email",
        "token"
      ],
      "properties": {
        "fullname": {
          "type": "string",
          "default": "",
          "title": "The fullname Schema",
          "examples": [
            "John Doe"
          ]
        },
        "email": {
          "type": "string",
          "default": "",
          "title": "The email Schema",
          "examples": [
            "parkifyOP@gmail.com"
          ]
        },
        "token": {
          "type": "string",
          "default": "",
